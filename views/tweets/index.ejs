<%- include('../partials/header') %>

<table id="list">
  <thead>
    <tr>
      <th>Tweet</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% tweets.forEach(function(m) { %>
      <tr>
        <td><%= m.title %></td>
        <td><%= m.releaseYear %></td>
        <td><a href="/tweets/<%= m._id %>">Tweet</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
